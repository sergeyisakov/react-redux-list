  "private": true,
  "devDependencies": {
    "gh-pages": "^1.0.0",
    "node-sass-chokidar": "0.0.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.7"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "enzyme": "^2.8.2",
    "normalizr": "^3.2.3",
